  "name": "@tekminewe/mint-ui",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Tek Min Ewe",
    "email": "ewe.tekmin@outlook.com",
    "url": "https://tekminewe.com"
  },
  "description": "Private UI component library for the monorepo workspace",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css",
    "./tailwind-plugin": {
      "import": "./dist/tailwind-plugin/index.js",
      "require": "./dist/tailwind-plugin/index.cjs",
      "types": "./dist/tailwind-plugin/index.d.ts"
    },
    "./*": {
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.cjs",
      "types": "./dist/*/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsc -b && vite build && cp package.json dist/ && cp README.md dist/",
    "build:watch": "vite build --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
